Задание 1

#include <stdio.h>
int main()
{
float a, b, c;
printf ("Введите число A ");
scanf ("%f", &a);
printf ("Введите число B ");
scanf ("%f", &b);
c = a;
a = b;
b = c;
printf ("Число А %f, Число В %f", a, b);
return 0;
}

Задание 2

#include <stdio.h>
int main()
{
float a, b, c, d, f;
printf ("Введите число A ");
scanf ("%f", &a);
printf ("Введите число B ");
scanf ("%f", &b);
printf ("Введите число C ");
scanf ("%f", &c);
d = a;
f = b;
a = c;
b = d;
c = f;
printf ("Число А %f, Число В %f, Число C %f", a, b, c);
return 0;
}

Задание 3

#include <stdio.h>
int main()
{
float a, b, c, d, f;
printf ("Введите число A ");
scanf ("%f", &a);
printf ("Введите число B ");
scanf ("%f", &b);
printf ("Введите число C ");
scanf ("%f", &c);
d = a;
f = b;
a = f;
b = c;
c = d;
printf ("Число А %f, Число В %f, Число C %f", a, b, c);
return 0;
}

Задание 4

#include <stdio.h>
#include <math.h>
int main()
{
float x, y;
printf ("Введите значение х ");
scanf ("%f", &x);
y = 3 * pow(x,6) - 6 * pow(x,2) - 7;
printf ("Значение функции равно %f", y);
return 0;
}

Задание 5

#include <stdio.h>
#include <math.h>
int main()
{
float x, y;
printf ("Введите значение х ");
scanf ("%f", &x);
y = 4 * pow((x-3),6) - 7 * pow((x-3),3) + 2;
printf ("Значение функции равно %f", y);
return 0;
}

